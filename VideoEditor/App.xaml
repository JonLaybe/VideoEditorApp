<Application x:Class="VideoEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VideoEditor"
             StartupUri="mvvm\View\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style\Themes\Dark.xaml"/>
                <ResourceDictionary Source="Style\Themes\Light.xaml"/>
                <ResourceDictionary Source="Style\DefaultButtonStyle.xaml"/>
                <ResourceDictionary Source="Style\InputStyle.xaml"/>
                <ResourceDictionary Source="Style\SliderStyle.xaml"/>
                <ResourceDictionary Source="mvvm\Utilites\DataTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
